{
  "manifest_version": 3,
  "name": "Selector Sync",
  "version": "0.1.0",
  "description": "Select DOM elements with selectors",
  "browser_specific_settings": {
    "gecko": {
      "id": "selector-sync@YusukeTakeuchi",
      "strict_min_version": "109.0"
    }
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon.png"
  },
  "options_ui": {
    "page": "options.html"
  },
  "icons": {
    "48": "icons/icon.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "scripting"
  ]
}
